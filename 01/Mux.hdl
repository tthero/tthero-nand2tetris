/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=selA);
    And(a=selA, b=a, out=resA);
    And(a=sel, b=b, out=resB);
    Or(a=resA, b=resB, out=out);
}